<table>
  <tr>
    <th>Title</th>
    <th>Completed</th>
    <th>Description</th>
    <th>Delete</th>
    <th>Update</th>
  </tr>
  {{#each tasks}}
  <tr>
    <td>{{title}}</td>
    <td>{{completed}}</td>
    <td>{{description}}</td>
    <td><button id="delete" -data-id= tasks._id> delete</button></td>
    <td><button id="update" -data-id= tasks._id>update</button></td>
  </tr>
  {{/each}}
</table>
